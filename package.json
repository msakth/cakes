    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "3.0.0",
    "image-webpack-loader": "4.3.1",
    "jest": "23.6.0",
    "npm-run-all": "^4.1.3",
    "open": "0.0.5",
    "react-hot-loader": "4.3.11",
    "react-test-renderer": "16.6.0",
    "rimraf": "2.6.2",
    "style-loader": "0.23.0",
    "webpack": "4.19.1",
    "webpack-cli": "3.1.0",
