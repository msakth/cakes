{
  "name": "cakes",
  "version": "0.1.0",
  "description": "cakes app to do CRUD operations",
  "author": "Muthu Sakthivel",
  "license": "MIT",
  "scripts": {
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "npm run start-prod",
    "clean-dist": "rimraf dist && mkdir dist",
    "start": "npm-run-all --parallel start-dev test:watch lint:watch",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev",
    "start-prod": "webpack -p --config=configs/webpack/prod",
    "test": "jest --coverage --config=configs/jest.json",
    "test:watch": "npm run test -- --watch ",
    "lint": "esw src configs --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "devDependencies": {
    "@babel/cli": "7.1.0",
